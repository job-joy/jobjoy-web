{
  "name": "progressionhub-web",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "webpack serve",
    "start": "webpack serve",
    "clean": "tsc --build --clean",
    "build": "npm-run-all --serial format build:ts",
    "build:ts": "tsc --build --pretty",
    "watch": "npm-run-all --parallel \"dev-server --watch\" \"build:ts --watch --preserveWatchOutput\"",
    "full-upgrade": "yarn upgrade-interactive --latest",
    "format": "yarn format:eslint && yarn format:prettier",
    "format:eslint": "eslint \"**/*.{js,ts,tsx}\" --fix",
    "format:prettier": "prettier \"**/*.{cjs,js,json,md,ts,tsx}\" --write",
    "test": "yarn build:ts && jest",
    "watch:test": "tsc --build test/tsconfig.json -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mersad998/progressionhub-web.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mersad998/progressionhub-web/issues"
  },
  "homepage": "https://github.com/mersad998/progressionhub-web#readme",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "@types/cypress": "^1.1.3",
    "@types/enzyme": "^3.10.9",
    "@types/jest": "^27.0.2",
    "@types/wait-on": "^5.3.1",
    "babel-loader": "^8.2.2",
    "html-webpack-plugin": "^5.3.2",
    "typescript": "^4.4.3",
    "webpack": "^5.55.1",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.3.0",
    "cypress":"^8.5.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-real-events": "^1.4.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "jest-enzyme": "^7.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "ts-jest": "^26.5.6"
  }
