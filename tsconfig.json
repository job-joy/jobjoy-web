{
    "compilerOptions": {
      "target": "es2019",
      "module": "es2015",
      "lib": ["es2020", "DOM", "DOM.Iterable"],
      "declaration": true,
      "declarationMap": true,
      "sourceMap": true,
      "inlineSources": true,
      "noImplicitReturns": true,
      "noFallthroughCasesInSwitch": true,
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "experimentalDecorators": true,
      "skipLibCheck": true,
      "outDir": "./",
      "importHelpers": true
    },
    "include": ["src/**/*"],
    "exclude": []
}
